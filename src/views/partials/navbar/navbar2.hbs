<script src="/js/web/new-navbar.js"></script>
<script>
    // Selecciona el input de búsqueda
    const searchInput = document.getElementById('search-app');

    // Función para filtrar las apps
    searchInput.addEventListener('input', function () {
        const filter = searchInput.value.toLowerCase(); // Obtiene el valor del input en minúsculas
        const apps = document.querySelectorAll('.app-icon'); // Selecciona todas las apps

        // Itera sobre cada app para mostrar u ocultar
        apps.forEach(app => {
            const appName = app.querySelector('span').textContent.toLowerCase(); // Obtiene el nombre de la app
            if (appName.includes(filter)) {
                app.style.display = ''; // Muestra la app
            } else {
                app.style.display = 'none'; // Oculta la app
            }
        });
    });
</script>