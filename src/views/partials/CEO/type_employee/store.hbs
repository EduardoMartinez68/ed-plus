<div class="tab-pane fade" id="navbar-store" role="tabpanel" aria-labelledby="navbar-store-tab">
    <div class="card">
        <div class="card-body">
            <label for=""><i class="fi-icon fi-sr-shopping-cart"></i> Tienda</label>
            <hr>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchPointSales" name="pointSales" valueCheck="{{app_point_sales}}">
                <label class="form-check-label" for="flexSwitchPointSales">App Punto de Venta</label>
            </div>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchViewProducts" name="viewProducts" valueCheck="{{view_products}}">
                <label class="form-check-label" for="flexSwitchViewProducts">App Productos</label>
            </div>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchEditProducts" name="editProducts" valueCheck="{{edit_products}}">
                <label class="form-check-label" for="flexSwitchEditProducts">Editar Productos</label>
            </div>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchDeleteProducts" name="deleteProducts" valueCheck="{{delete_products}}">
                <label class="form-check-label" for="flexSwitchDeleteProducts">Eliminar Productos</label>
            </div>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchViewProducts" name="viewInventory" valueCheck="{{view_inventory}}">
                <label class="form-check-label" for="flexSwitchViewProducts">App Inventario</label>
            </div>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchEditProducts" name="editInventory" valueCheck="{{edit_inventory}}">
                <label class="form-check-label" for="flexSwitchEditProducts">Editar inventario</label>
            </div>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="add_lot" name="add_lot" valueCheck="{{add_lot}}">
                <label class="form-check-label" for="add_lot">Agregar Lotes & Almacenes</label>
            </div>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="edit_lot" name="edit_lot" valueCheck="{{edit_lot}}">
                <label class="form-check-label" for="edit_lot">Editar Lotes & Almacenes</label>
            </div>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="delete_lot" name="delete_lot" valueCheck="{{delete_lot}}">
                <label class="form-check-label" for="delete_lot">Eliminar Lotes & Almacenes</label>
            </div>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchAddOffers" name="addOffers" valueCheck="{{add_offers}}">
                <label class="form-check-label" for="flexSwitchAddOffers">Agregar Ofertas</label>
            </div>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchEditOffers" name="editOffers" valueCheck="{{edit_offers}}">
                <label class="form-check-label" for="flexSwitchEditOffers">Editar Ofertas</label>
            </div>
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchDeleteOffers" name="deleteOffers" valueCheck="{{delete_offers}}">
                <label class="form-check-label" for="flexSwitchDeleteOffers">Eliminar Ofertas</label>
            </div>
        </div>
